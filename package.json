{
  "private": false,
  "name": "react-babylonjs",
  "version": "0.0.7",
  "description": "Integration component and middleware for BabylonJS with React/Redux",
  "homepage": "https://github.com/brianzinn/react-babylonJS/",
  "repository": "brianzinn/react-babylonJS",
  "author": "Brian Zinn",
  "contributors": [],
  "license": "MIT",
  "keywords": [],
  "main": "dist/index.js",
  "jsnext:main": "index.es.js",
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-react-jsx"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base"
  },
  "dependencies": {
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babylonjs": "^2.5.0",
    "react": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^4.0.0-canary.1",
    "coveralls": "^2.11.14",
    "del": "^2.2.2",
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^2.0.0-pre.3"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo brianzinn/react-babylonJS",
    "start": "easystatic start docs"
  }
}
